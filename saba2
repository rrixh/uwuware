    glass.Transparency = 0
    if glass.CanCollide then
glass.BrickColor = BrickColor.new("Pink")
    else
glass.BrickColor = BrickColor.new("Really red")
        glass.CanCollide = true
        glass.CanTouch = false
    end
end
end;
