local finish, tile, glassParts;

uwu:AddButton({text = "Reveal Korrect Path", callback = function()
for i = 1, 30 do
tile = workspace.obby:GetChildren()[i];
glassParts = tile.glassParts:GetChildren();

    for _, part in ipairs(glassParts) do
part.Transparency = -3
        if part.Name == "leftFakeGlass" or part.Name == "rightFakeGlass" then
 part.BrickColor = BrickColor.new("Really red")
else
part.BrickColor = BrickColor.new("Lime green")
        end
    end
   end;
end})

uwu:AddButton({text = "Tp to end", callback = function()
finish = game:GetService("Workspace").finish.finishLineArea.CFrame;
hrp.CFrame = finish;
end})

uwu:AddButton({text = "Instant Win", callback = function()
finish = game:GetService("Workspace").finish.finishLineArea
_G.touxh(finish,1);
end})
