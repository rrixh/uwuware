local Telegram = ("https://t.me/sIipped")

-- windows
local wizard = wizRRIXH:NewWindow("test ");
local autofarmWindow = wizRRIXH:NewWindow("Autofarm");
-- separators/sektions
local annoy = wizard:NewSection("Annoying (all players)");
local target = wizard:NewSection("Target ah Bitxh");
local wiz = wizard:NewSection("Kredits üç≠")
local autofarm = autofarmWindow:NewSection("farm")
local invalid = {"", " ", "  ", "   ", "    ", "     ", "      ", "       ", "        "}
-- variables
local players = game:GetService("Players")
local hitting, spamming, bringTarget, autoFarmStompEnabled = false, false, false, false
local hitConnection, spamConnection, bringTargetConnection, currentTarget, targetPlayer, autofarmStompConnection;
local distanceBehind, distanceInFront = 4.15, -4.22

-- functions
local function hit()
local args = { [1] = 1 }
_G.rep.PUNCHEVENT:FireServer(unpack(args))
end;
local function grab()
_G.rep.JALADADEPELOEVENT:FireServer()
end;
local function stomp()
_G.rep.STOMPEVENT:FireServer()
end;
local function killPlayers()
hit(); grab();
end;

local function findNearestPlayer()
local nearestPlayer = nil
local shortestDistance = math.huge;
for _, otherPlayer in ipairs(_G.gplrs) do
if otherPlayer ~= players.LocalPlayer and otherPlayer.Character and otherPlayer.Character:FindFirstChild("HumanoidRootPart") then
local distance = (players.LocalPlayer.Character.HumanoidRootPart.Position - otherPlayer.Character.HumanoidRootPart.Position).Magnitude
if distance < shortestDistance then
shortestDistance = distance
nearestPlayer = otherPlayer
end; end; end
return nearestPlayer
end;

local function teleportBehind(target)
if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
local targetCFrame = target.Character.HumanoidRootPart.CFrame
local newPosition = targetCFrame * CFrame.new(0, 0, distanceBehind)
players.LocalPlayer.Character.HumanoidRootPart.CFrame = newPosition
end;
end;

local function bringPlayer(target)
if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
local localPlayerCFrame = players.LocalPlayer.Character.HumanoidRootPart.CFrame
local newPosition = localPlayerCFrame * CFrame.new(0, 0, distanceInFront)
target.Character.HumanoidRootPart.CFrame = newPosition
end;end;

local function findTargetPlayer(name)
for _, player in ipairs(players:GetPlayers()) do if player.DisplayName:lower():find(name:lower()) or player.Name:lower():find(name:lower()) then
return player
end;end; return nil
end;

-- annoying all
annoy:Toggle("Auto Kill (nearest)", function(toggleOn)
hitting = toggleOn;
if hitting then
hitConnection = _G.renderstepped:Connect(function()
if currentTarget and currentTarget.Character and currentTarget.Character:FindFirstChild("Humanoid") then
if currentTarget.Character.Humanoid.Health <= 0 then
local targetCFrame = currentTarget.Character.HumanoidRootPart.CFrame
players.LocalPlayer.Character.HumanoidRootPart.CFrame = targetCFrame * CFrame.new(0, 5, 0) -- Teleport on top
stomp();
else
teleportBehind(currentTarget)
killPlayers();
end
else
currentTarget = findNearestPlayer()
end
end)
else
if hitConnection then
hitConnection:Disconnect()
hitConnection = nil
currentTarget = nil;
end;
end;
end)

annoy:Toggle("Ghost Hit All (lag server)", function(toggleOn)
spamming = toggleOn;
if spamming then
spamConnection = _G.renderstepped:Connect(function()
for _, otherPlayer in ipairs(players:GetPlayers()) do
if otherPlayer ~= players.LocalPlayer and otherPlayer.Character and otherPlayer.Character:FindFirstChild("HumanoidRootPart") then
bringPlayer(otherPlayer)
hit()
end; end; end)
else
if spamConnection then
spamConnection:Disconnect()
spamConnection = nil
end
end
end)

-- annoy target
target:Txtbox("player name (shortened)", function(targetName)
if table.find(invalid, targetName) then
print"name kannot be blank" return end;
if targetName and targetName ~= "" then
targetPlayer = findTargetPlayer(targetName)
if not targetPlayer then
print"the bitxh was not found"
targetPlayer = nil
end
else
print"text kant be empty"
targetPlayer = nil;
end
end)

target:Toggle("Auto Kill (target)", function(toggleOn)
bringTarget = toggleOn;
if bringTarget then
bringTargetConnection = _G.heartbeat:Connect(function()
if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("Humanoid") then
if targetPlayer.Character.Humanoid.Health > 0 then
teleportBehind(targetPlayer);
killPlayers();
end
else
print("Target not valid or dead");
end;
end)
else
if bringTargetConnection then
bringTargetConnection:Disconnect()
bringTargetConnection = nil
end;
end
end)

target:Toggle("Ghost hit Target", function(toggleOn)
bringTarget = toggleOn;
if bringTarget then
bringTargetConnection = _G.heartbeat:Connect(function()
if targetPlayer then
bringPlayer(targetPlayer);
hit();
   end;
end)
else
if bringTargetConnection then
bringTargetConnection:Disconnect()
bringTargetConnection = nil
   end;
end
end)

-- autofarm tab
autofarm:Toggle("Auto Stomp", function(toggleOn)
autoFarmStompEnabled = toggleOn
if autoFarmStompEnabled then
autofarmStompConnection = _G.heartbeat:Connect(function()
local deadPlayerFound = false
for _, player in ipairs(players:GetPlayers()) do
if player ~= players.LocalPlayer and player.Character then
local humanoid = player.Character:FindFirstChild("Humanoid")
if humanoid and humanoid.Health <= 0 then
local deadplrHRP = player.Character:FindFirstChild("HumanoidRootPart")
if deadplrHRP then
local prevPos = _G.hrp.CFrame
_G.hrp.CFrame = deadplrHRP.CFrame
stomp()
deadPlayerFound = true
wait(.1)
_G.hrp.CFrame = prevPos --Teleport back
end
end
end
end
if not deadPlayerFound then
wait(.41)
end
end)
else
if autofarmStompConnection then
autofarmStompConnection:Disconnect()
autofarmStompConnection = nil
end;
end
end)

