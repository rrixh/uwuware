local wizRRIXH = loadstring(game:HttpGet('https://github.com/rrixh/uwuware/raw/refs/heads/main/lib/wizRRIXH'))();

-- MY LINKS --
local RobloxLink = ("https://www.roblox.com/users/3001347724/profile")
