end;

local uwuRRIXH = loadstring(game:HttpGet("https://raw.githubusercontent.com/rrixh/uwuware/main/mobileware", true))();
local uwu = uwuRRIXH:CreateWindow("The Glass Bridge")

uwu:AddButton({text = "Instant Win", callback=function()
_G.touxh(Workspace.GameLogicHandler.FinishArea)
end})

uwu:AddButton({text = "Reveal Korrect Path", callback=function()
glass = Workspace.GlassBridges.CorrectGlass:GetChildren();
for i,v in ipairs(glass) do
v.Transparency = .5
v.BrickColor=BrickColor.new("Lime green")
    end;
end})

uwu:AddLabel({text ="RBX @lulaslollipop üç≠"})
