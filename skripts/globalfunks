    _G.plr = game.Players.LocalPlayer
    _G.hrp = _G.plr.Character and _G.plr.Character:FindFirstChild("HumanoidRootPart");
    if not _G.hrp then return end;
    _G.hrp.CFrame = part.CFrame
    if duration then
    task.wait(duration)
        end;
    end;

