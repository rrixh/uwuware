--getgenv().slots = 11
getgenv().toolbarsize = 40.6
local bgkolor = Color3.fromRGB(100, 100, 100)
local yellow = Color3.fromRGB(255, 255, 0)
local green = Color3.fromRGB(0, 255, 0)
